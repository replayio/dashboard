{
  "name": "replay-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 8080",
    "preprod": "NEXT_PUBLIC_API_URL=http://graphql-api.pre-prod.replay.prod/v1/graphql next dev -p 8080",
    "graphql": "pnpm graphql-schema && pnpm graphql-types",
    "graphql-schema": "gq https://graphql.replay.io/v1/graphql -H \"X-Hasura-Admin-Secret: $HASURA_KEY\" --introspect > schema.graphql",
    "graphql-types": "graphql-codegen --config codegen.ts",
    "lint": "next lint",
    "prettier": "prettier . --write",
    "prettier:ci": "prettier . --check",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:debug": "playwright test --project=chromium --workers 1 --headed",
    "test:e2e:record": "REPLAY_PLAYWRIGHT_FIXTURE=1 playwright test --project=replay-chromium",
    "typescript": "tsc --noEmit",
    "typescript:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.9.6",
    "@auth0/nextjs-auth0": "^3.5.0",
    "@replayio/overboard": "^0.4.1",
    "@stripe/react-stripe-js": "^2.6.2",
    "@stripe/stripe-js": "^3.0.10",
    "chalk": "^4",
    "cookie": "^0.6.0",
    "date-fns": "^3.3.1",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "pretty-ms": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-resizable-panels": "^2.0.1",
    "use-context-menu": "^0.4.13",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "1.38.1",
    "@replayio/playwright": "3.0.0-alpha.3",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.2.1",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/uuid": "^9.0.8",
    "apollo": "^2.34.0",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.4",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "graphqurl": "^1.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "1.38.1",
    "postcss": "^8.4.33",
    "prettier": "3.2.4",
    "strip-ansi": "^6",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  }
}
